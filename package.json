  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "vercel-build": "corepack enable && pnpm install --no-frozen-lockfile && pnpm run build",
    "test": "echo 'Tests not configured'",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next dist node_modules",
    "setup": "corepack enable && pnpm install && pnpm run build",
    "ci": "pnpm run lint && pnpm run test && pnpm run build",
    "validate-lockfile": "pnpm install --frozen-lockfile || echo 'Lockfile is outdated - run pnpm install to update'",
    "regenerate-lockfile": "if exist pnpm-lock.yaml del pnpm-lock.yaml && pnpm install",
    "postinstall": "echo 'Lockfile validated with pnpm@10.0.0'"
  },
